package GeneralTools;

/**
 * @author Riccardo
 *
 */

public abstract class In_Out<T> {

	protected String _variable;
	protected T _value;
	protected int _label;
	
// Constructors:
	public In_Out() {
	}
	
// Getters:	
	public String getVariable() {
		return _variable;
	}
	
	public T getValue() {
		return _value;
	}
	
	public int getLabel() {
		return _label;
	}
	
// Setters:	
	public void setVariable(String variableString) {
		_variable = variableString;
	}
	
	public void setValue(T t) {
		_value = t;
	}
	
	public void setLabel(int label) {
		_label = label;
	}
	
// Printers:
	public abstract String print();
		
}
