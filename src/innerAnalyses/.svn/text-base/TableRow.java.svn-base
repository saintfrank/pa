package innerAnalyses;

import java.util.Set;

public class TableRow {
	
	private int _id;
	private String singleLeft;	//This is the left-hand side of the equation.
	private Set<String> _right;	//This is the right-hand side of the equation.
	
	public TableRow(){
		
	}
	
	public TableRow(int id, String left, Set<String> right){
		
		this._id = id;
		this.singleLeft = left;
		this._right = right;
	}

	public int getId() {
		return _id;
	}

	public void setId(int _id) {
		this._id = _id;
	}

	public String getLeft() {
		return singleLeft;
	}

	public void setLeft(String _left) {
		this.singleLeft = _left;
	}

	public Set<String> getRight() {
		return _right;
	}

	public void setRight(Set<String> _right) {
		this._right = _right;
	}
	
	public String toString(){
		
		return "ID: "+ _id + " Left: " + singleLeft + " Right: " + _right;
	}
}
